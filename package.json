{
  "name": "scrum-poker",
  "version": "1.0.0",
  "description": "Scrum planing remotely never be so easy.",
  "repository": "https://github.com/StarfruitTree/scrum-poker.git",
  "homepage": "https://starfruittree.github.io/scrum-poker/",
  "author": "StarfruitTree",
  "license": "ISC",
  "private": true,
  "keywords": [],
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.dev.ts",
    "build": "webpack --mode production --config webpack.prod.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage && open ./coverage/lcov-report/index.html",
    "deploy": "gh-pages -d dist"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^@scrpoker(.*)": "<rootDir>/src/$1",
      "\\.(scss|css)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.3",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-jest": "^26.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "mini-css-extract-plugin": "^0.10.0",
    "prettier": "^2.1.0",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.2"
  },
  "dependencies": {
    "@microsoft/signalr": "^3.1.8",
    "@types/react-router-dom": "^5.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0"
  }
}
